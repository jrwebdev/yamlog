{
  "name": "yamlog",
  "version": "0.0.8",
  "main": "index.js",
  "repository": "git@github.com:jrwebdev/yamlog.git",
  "author": "jrwebdev <web@jravenscroft.co.uk>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "yamlog": {
    "unstable": true,
    "publish": {
      "dir": "dist"
    },
    "commit": {
      "cmd": "yarn commit"
    }
  },
  "bin": {
    "yamlog": "./bin/yamlog.js"
  },
  "scripts": {
    "log": "ts-node src/bin/yamlog",
    "bump": "ts-node src/bin/yamlog bump-version",
    "changes": "ts-node src/bin/yamlog changes",
    "test": "jest --watch",
    "test:all": "jest --coverage",
    "precommit": "ts-node src/bin/yamlog use-commit",
    "commit": "ts-node src/bin/yamlog commit",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "release": "ts-node src/bin/yamlog publish"
  },
  "dependencies": {
    "chalk": "^2.2.0",
    "commander": "^2.11.0",
    "execa": "^0.8.0",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "inquirer": "^3.3.0",
    "js-yaml": "^3.10.0",
    "listr": "^0.12.0",
    "moment": "^2.19.1",
    "pify": "^3.0.0",
    "ramda": "^0.25.0",
    "yargs": "^9.0.1",
    "yargs-parser": "^8.0.0"
  },
  "devDependencies": {
    "@types/commander": "^2.11.0",
    "@types/execa": "^0.7.1",
    "@types/fs-extra": "^4.0.2",
    "@types/glob": "^5.0.33",
    "@types/inquirer": "^0.0.35",
    "@types/jest": "^21.1.3",
    "@types/js-yaml": "^3.9.1",
    "@types/mock-fs": "^3.6.30",
    "@types/mockdate": "^2.0.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^8.0.41",
    "@types/pify": "^3.0.0",
    "@types/ramda": "^0.24.17",
    "@types/yargs": "^8.0.2",
    "jest": "^21.2.1",
    "mock-fs": "^4.4.2",
    "mockdate": "^2.0.2",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "ts-jest": "^21.1.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3"
  }
}